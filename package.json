{
  "name": "vercel-storage",
  "version": "0.0.1",
  "description": "The monorepo for everything Postgres at Vercel",
  "homepage": "https://vercel.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/storage.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "turbo build",
    "lint": "turbo lint -- --max-warnings=0",
    "prepare": "husky install",
    "prettier-check": "prettier --check .",
    "publint": "turbo publint",
    "release": "pnpm build && changeset publish",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "version-packages": "changeset version && pnpm i --no-frozen-lockfile && git add ."
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "turbo lint -- --max-warnings=0 --fix"
    ]
  },
  "prettier": "@vercel/style-guide/prettier",
  "devDependencies": {
    "@changesets/cli": "2.25.0",
    "@vercel/style-guide": "4.0.2",
    "eslint": "8.25.0",
    "eslint-config-custom": "workspace:*",
    "husky": "8.0.1",
    "jest": "29.2.1",
    "lint-staged": "13.0.3",
    "prettier": "2.8.8",
    "ts-jest": "29.0.3",
    "turbo": "latest",
    "typescript": "^4.8.0"
  },
  "packageManager": "pnpm@8.4.0",
  "engines": {
    "node": ">=16.14"
  }
}
