{
  "name": "@vercel/edge-config",
  "version": "0.1.0-canary.1",
  "description": "",
  "repository": "vercel/edge-config",
  "main": "./dist/index.cjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=14.6"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "pnpm run test-nodejs && pnpm run test-edge",
    "test-nodejs": "jest --env node",
    "test-edge": "jest --env @edge-runtime/jest-environment",
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "eslint-check": "eslint --max-warnings=0 .",
    "prettier-check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "release": "pnpm build && changeset publish",
    "version-packages": "changeset version"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "prettier": "@vercel/style-guide/prettier",
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@changesets/cli": "2.25.0",
    "@edge-runtime/jest-environment": "1.1.0-beta.31",
    "@types/jest": "29.0.2",
    "@types/node": "18.7.18",
    "@vercel/style-guide": "3.0.0",
    "eslint": "8.23.1",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.1",
    "tsup": "6.2.3",
    "typescript": "4.8.3"
  }
}
